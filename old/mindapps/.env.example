# Mind Apps Configuration
# Copy this file to .env and fill in your values

# Base URL for the API (include trailing slash)
API_BASE_URL=https://thefeather.ink/apps/

# Endpoint for fetching app list JSON
APPS_JSON_ENDPOINT=mind.json

# Endpoint for sending download analytics
DATA_ENDPOINT=data.php

# Secret key for authenticating analytics requests
SECRET_KEY=your_secret_key_here
